{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run C# File",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["run", "--project", "${workspaceFolder}/temp_run/TempRun"],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "build-current-file",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "requireExactSource": false,
            "enableStepFiltering": false,
            "logging": {
                "moduleLoad": false,
                "programOutput": true,
                "exceptions": false
            }
        }
    ]
}